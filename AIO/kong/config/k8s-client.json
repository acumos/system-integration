{
  "strip_uri": false,
  "name": "k8s-client",
  "http_if_terminated": false,
  "https_only": true,
  "upstream_url": "http://kubernetes-client-service:8082",
  "uris": [
    "/package"
  ],
  "preserve_host": false,
  "upstream_connect_timeout": 60000,
  "upstream_read_timeout": 600000,
  "upstream_send_timeout": 600000,
  "retries": 5
}
